{
	"name": "@rbxts/forge",
	"version": "0.4.1",
	"description": "An App Manager for Vide",
	"main": "out/init.lua",
	"types": "out/index.d.ts",
	"packageManager": "bun@1.3.1",
	"scripts": {
		"compile": "rbxtsc && node -e \"const fs=require('fs');const f='out/index.d.ts';fs.writeFileSync(f,'/// <reference path=\\\"./types/global.d.ts\\\" />\\n'+fs.readFileSync(f,'utf8'))\"",
		"compile-all": "bunx run compile && rm -rf test/node_modules/@rbxts/forge && cp -r out test/node_modules/@rbxts/forge"
	},
	"keywords": [
		"roblox-ts",
		"vide"
	],
	"author": "Loner1536",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Loner1536/Forge"
	},
	"bugs": {
		"url": "https://github.com/Loner1536/Forge/issues"
	},
	"files": [
		"out",
		"!**/*.tsbuildinfo",
		"!**/*.spec.lua",
		"!**/*.spec.d.ts"
	],
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@rbxts/services": "^1.6.0",
		"@rbxts/set-timeout": "^1.1.2",
		"concurrently": "^9.2.1",
		"@rbxts/vide": "*",
		"turbo": "^2.8.10"
	},
	"devDependencies": {
		"@rbxts/compiler-types": "3.0.0-types.0",
		"@biomejs/biome": "^2.3.7",
		"@rbxts/types": "^1.0.891",
		"roblox-ts": "3.0.0",
		"typescript": "^5.9.3"
	}
}
